import java.util.Scanner;


public class P02_TriangleArea {

	public static void main(String[] args) {
		Scanner in = new Scanner(System.in);
		String input = "";
		for (int i = 0; i < 3; i++) {
			input = in.nextLine();
		}
		
		String[] coordinates = input.split(" ");
		
		System.out.println(coordinates[1]);
		
	}

}
