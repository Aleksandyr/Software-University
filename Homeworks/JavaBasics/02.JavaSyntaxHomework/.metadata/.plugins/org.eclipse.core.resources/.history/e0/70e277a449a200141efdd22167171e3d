import java.util.Scanner;


public class P07_CountOfBitsOne {

	public static void main(String[] args) {
		Scanner in = new Scanner(System.in);
		
		int number = in.nextInt();
		
		int count = 0;
		for (int i = 0; i < 32; i++) {
			int digit = (number >> i) & 1;
			if (digit == 1) {
				count++;
			}
		}
		
		System.out.println(count);
	}

}
