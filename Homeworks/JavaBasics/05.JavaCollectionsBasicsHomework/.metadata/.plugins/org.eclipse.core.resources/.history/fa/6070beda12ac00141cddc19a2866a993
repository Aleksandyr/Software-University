		TreeMap<String, Integer> result = new TreeMap<String, Integer>();
		
		int counter = 0;
		int max = Integer.MIN_VALUE;
		for (String string : text) {
			
		}
		
		for (String key : result.keySet()) {
			System.out.println(key + "->" + result.get(key));
		}
	}

}
