import java.util.Scanner;


public class P05_AngleUnitConverter {

	public static void main(String[] args) {
		Scanner in = new Scanner(System.in);
		
		int n = in.nextInt();
		
		for (int i = 0; i <= n; i++) {
			String input = in.nextLine();
			String[] numberWihtMesure = input.split(" ");
			double number = Integer.parseInt(numberWihtMesure[0]);
			String mesure = numberWihtMesure[1];
			
			if (mesure == "deg") {
				System.out.println(ConvertDegreesToRadians(number) + " " + mesure);
			} else {
				System.out.println(ConvertRadiansToDegrees(number) + " " + mesure);
			}
		}
	}
	
	public static double ConvertRadiansToDegrees(double radians){
		return radians * (180 / Math.PI);
	}
	
	public static double ConvertDegreesToRadians(double degrees){
		return degrees / (180 / Math.PI);
	}

}
