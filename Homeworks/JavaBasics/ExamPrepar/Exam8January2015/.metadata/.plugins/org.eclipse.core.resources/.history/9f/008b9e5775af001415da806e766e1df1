import java.util.Scanner;


public class P01_GandalfsStash {

	public static void main(String[] args) {
		Scanner in = new Scanner(System.in);
		System.out.println(3);
		int firstMood = in.nextInt();
		String gandalfsFood = in.nextLine();
		String[] gandalfsListOfFood = gandalfsFood.split("[^a-zA-z]+");
		for (int i = 0; i < gandalfsListOfFood.length; i++) {
			System.out.println(gandalfsListOfFood[i]);
		}
	}

}
