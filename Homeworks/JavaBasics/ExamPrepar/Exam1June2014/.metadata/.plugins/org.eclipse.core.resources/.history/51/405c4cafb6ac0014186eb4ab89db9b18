import java.util.Scanner;


public class P02_SumCards {

	public static void main(String[] args) {
		Scanner scanner = new Scanner(System.in);
		
		String input = scanner.nextLine().trim();
		String[] facesFrom1to10 = input.split("[^0-9]+");
		String[] facesFromJtoA = input.split("[^JQKA]+");
		
		int[] numbers = new int[facesFrom1to10.length];
		for (int i = 0; i < numbers.length; i++) {
			numbers[i] = Integer.parseInt(facesFrom1to10[i]);
		}
		
		int digitSum = 0;
		int currSum = 0;
		int prevValue = -1;
		for (int i = 0; i < numbers.length - 1; i++) {
			currSum += numbers[i];
			prevValue = numbers[i + 1];
			if (numbers[i] != prevValue) {
				digitSum += currSum * 2;
				currSum = 0;
			}
		}
		
		System.out.println(digitSum);
	}

}
